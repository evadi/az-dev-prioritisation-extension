{
  "manifestVersion": 1,
  "version": "1.0.1",
  "name": "Prioritisation: A three factor prioritisation system",
  "scopes": [
    "vso.work",
    "vso.work_write"
  ],
  "description": "An Azure DevOps extension to help calculate a Business Value score for prioritising work items",
  "publisher": "orchidsoft",
  "id": "az-dev-prioritisation-extension",
  "icons": {
    "default": "marketplace/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Work Items",
    "Priorization",
    "Agile",
    "Backlog",
    "Product management"
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE.md"
    }
  },
  "links": {
    "repository": {
      "uri": "https://github.com/evadi/az-dev-prioritisation-extension"
    },
    "learn": {
      "uri": "https://github.com/evadi/az-dev-prioritisation-extension"
    },
    "support": {
      "uri": "https://github.com/evadi/az-dev-prioritisation-extension/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/evadi/az-dev-prioritisation-extension"
  },
  "categories": [
    "Azure Boards"
  ],
  "contributions": [
    {
      "id": "rice-work-item-form-observer",
      "type": "ms.vss-work-web.work-item-notifications",
      "description": "Update the 'Business Value' field when other fields on the form change.",
      "targets": [
        "ms.vss-work-web.work-item-form"
      ],
      "properties": {
        "uri": "src/rice.html"
      }
    },
    {
      "id": "rice-settings-hub",
      "type": "ms.vss-web.hub",
      "description": "Collection level settings experience for managing fields used to calculate Business Value.",
      "targets": [
        "ms.vss-web.collection-admin-hub-group"
      ],
      "properties": {
        "name": "Prioritisation Settings",
        "uri": "src/settings.html"
      }
    }
  ],
  "files": []
}